    "eslint": "7.18.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-prettier": "3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1"
  },
  "jest": {
    "globals": {},
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(tests?/.+(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
